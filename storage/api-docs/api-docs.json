{
    "openapi": "3.0.0",
    "info": {
        "title": "Ma premi√®re API",
        "version": "0.1"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "API de l'agence immobiliere Sous Mon Toit"
        }
    ],
    "paths": {
        "/contract": {
            "get": {
                "tags": [
                    "Contracts"
                ],
                "summary": "list all contracts",
                "description": "Retourne la liste des contrats",
                "operationId": "getContractsList",
                "responses": {
                    "200": {
                        "description": "A list with contracts",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "Contract"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "default": {
                        "description": "une erreur \"inattendue\""
                    }
                }
            }
        }
    }
}